/var/log/ampache/ampache.log {
	daily
	missingok
	rotate 5
	compress
	olddir=old
	shred
	postrotate
		chown -R www-data:www-data /var/log/ampache
	endscript
}

/var/log/ampache/ampache-catalog.log {
	daily
	missingok
	rotate 5
	compress
	olddir=old
	shred
	postrotate
		chown -R www-data:www-data /var/log/ampache
	endscript
}
